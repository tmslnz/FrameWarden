#tag ClassProtected Class Node_ModoTCPSocketInherits TCPSocket	#tag Event		Sub Connected()		  WriteToLog "Connected to Modo", LogFuncs.Netwk		  me.ReadyForNextCommand = True		End Sub	#tag EndEvent	#tag Event		Sub DataAvailable()		  data = me.ReadAll		  		  WriteToLog "Data is: " + data, LogFuncs.Netwk		  		  if data = "||ack||" or data = "||init||" then		    //me.ReadyForNextCommand = True		    me.LastResponse = Ticks()		  end if		End Sub	#tag EndEvent	#tag Property, Flags = &h0		data As String	#tag EndProperty	#tag Property, Flags = &h0		LastResponse As Integer	#tag EndProperty	#tag Property, Flags = &h0		NextLaunchAttempt As Int32	#tag EndProperty	#tag Property, Flags = &h0		ReadyForNextCommand As Boolean	#tag EndProperty	#tag ViewBehavior		#tag ViewProperty			Name="Address"			Visible=true			Group="Behavior"			Type="String"			InheritedFrom="TCPSocket"		#tag EndViewProperty		#tag ViewProperty			Name="data"			Group="Behavior"			Type="String"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			Type="Integer"			InheritedFrom="TCPSocket"		#tag EndViewProperty		#tag ViewProperty			Name="LastResponse"			Group="Behavior"			InitialValue="0"			Type="Integer"		#tag EndViewProperty		#tag ViewProperty			Name="Left"			Visible=true			Group="Position"			InheritedFrom="TCPSocket"		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			InheritedFrom="TCPSocket"		#tag EndViewProperty		#tag ViewProperty			Name="Port"			Visible=true			Group="Behavior"			InitialValue="0"			Type="Integer"			InheritedFrom="TCPSocket"		#tag EndViewProperty		#tag ViewProperty			Name="ReadyForNextCommand"			Group="Behavior"			InitialValue="0"			Type="Boolean"		#tag EndViewProperty		#tag ViewProperty			Name="Super"			Visible=true			Group="ID"			InheritedFrom="TCPSocket"		#tag EndViewProperty		#tag ViewProperty			Name="Top"			Visible=true			Group="Position"			InheritedFrom="TCPSocket"		#tag EndViewProperty	#tag EndViewBehaviorEnd Class#tag EndClass