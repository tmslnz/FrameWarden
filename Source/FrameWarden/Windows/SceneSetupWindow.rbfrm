		  Dim uvNamesArray(-1) As String		  Dim nameTaken As Boolean		  		  VMAPMenu.DeleteAllRows		  		  // This will go through the UV maps and only find one of each		  for i = 0 to TheFarm.Scenes(SceneIndex).VMAP.Ubound		    curMapName = TheFarm.Scenes(SceneIndex).VMAP(i)		    nameTaken = false		    		    for nameIndex = 0 to uvNamesArray.Ubound		      if uvNamesArray(nameIndex) = curMapName then		        nameTaken = true		      end if		    next		    		    if nameTaken = false then		      uvNamesArray.Append curMapName		    end if		  next		  		  		  // This sorts the array alphabetically		  for i = 0 to uvNamesArray.Ubound-1		    for i2 = i+1 to uvNamesArray.Ubound		      if uvNamesArray(i) < uvNamesArray(i2) then		        // swap the maps		        curMapName = uvNamesArray(i)		        uvNamesArray(i) = uvNamesArray(i2)		        uvNamesArray(i2) = curMapName		      end if		    next		  next		  		  		  // This one adds them to our menu		  for i = 0 to uvNamesArray.Ubound		    VMAPMenu.AddRow uvNamesArray(i)		  next		  		  		  // if there are two or more elements in the array, let's display an option to bake all maps		  if uvNamesArray.Ubound > 0 then		    // Add a bake all command		    VMAPMenu.AddRow "* Bake All Maps"		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function MakeNewScenePerUV(queued As Boolean) As boolean		  Dim i, i2 as integer		  Dim selectedFolder As FolderItem		  Dim newScene As FarmScene		  		  // Make sure the user selects a folder to render the images to		  selectedFolder = SelectFolder()		  		  if selectedFolder = Nil then		    return false		  end if		  		  // Set certain fields that are consistent with all VMAPs		  UseCustomLayeredPathCheckBox.Value = true		  FirstFrameField.text = "1"		  LastFrameField.text = "1"		  		  // Go through each UV and make a new render scene		  VMAPMenu.ListIndex = 0		  		  // Get the file path		  #if TargetWin32		    NewOutputNameField.Text = selectedFolder.AbsolutePath + VMAPMenu.text		  #elseif TargetMacOS		    NewOutputNameField.Text = ConvertToShellPath(selectedFolder.AbsolutePath) + VMAPMenu.text		  #endif		  		  if queued then		    TheFarm.Scenes(SceneIndex).Status = FarmScene.snQueued		  end if		  		  me.UpdateSceneData(queued)		  QueueWindow.UpdateSceneList()		  		  // Go through each UV and make a new scene to Bake		  for i = 1 to VMAPMenu.ListCount - 2		    if TheFarm.AddScene(TheFarm.Scenes(SceneIndex).File) then		      SceneSetupWindow.SceneIndex = QueueWindow.SceneList.LastIndex		      		      QueueWindow.UpdateSceneList()		      		      // Go to the next VMAP and set the output location		      VMAPMenu.ListIndex = i		      		      // Get the file path		      #if TargetWin32		        NewOutputNameField.Text = selectedFolder.AbsolutePath + VMAPMenu.text		      #elseif TargetMacOS		        NewOutputNameField.Text = ConvertToShellPath(selectedFolder.AbsolutePath) + VMAPMenu.text		      #endif		      		      // Find the correct UV map		      for i2 = 0 to TheFarm.Scenes(SceneIndex).VMAP.Ubound		        if VMAPMenu.Text = TheFarm.Scenes(SceneIndex).VMAP(i2) then		          TheFarm.Scenes(SceneIndex).VMAPIndex = i2		        end if		      next		      		      TheFarm.Scenes(SceneIndex).Bake = true		      		      // Finally, let's update the general scene data		      me.UpdateSceneData(queued)		      QueueWindow.UpdateSceneList()		      		      // Let's refresh our windows so the user will see some progress		      self.Refresh()		      QueueWindow.Refresh()		      		    end if		  next		  		  return true		End Function	#tag EndMethod	#tag Method, Flags = &h0		Sub UpdateSceneData(queued As Boolean)		  // Scene Frame Range		  if BakeUVsBox.value then		    TheFarm.Scenes(SceneIndex).FirstFrame = 1		    TheFarm.Scenes(SceneIndex).LastFrame = 1		    TheFarm.Scenes(SceneIndex).CurrentFrame = 1		    TheFarm.Scenes(SceneIndex).FrameStep = 1		    		  else		    TheFarm.Scenes(SceneIndex).FirstFrame = Val(FirstFrameField.Text)		    TheFarm.Scenes(SceneIndex).LastFrame = Val(LastFrameField.Text)		    TheFarm.Scenes(SceneIndex).CurrentFrame = TheFarm.Scenes(SceneIndex).FirstFrame		    TheFarm.Scenes(SceneIndex).FrameStep = Val(FrameStepField.Text)		    		    TheFarm.Scenes(SceneIndex).VMAPIndex = -1		    		  end if		  		  TheFarm.Scenes(SceneIndex).SetScene(TheFarm.Scenes(SceneIndex).File)		  		  // Just in case the user entered 0, we don't want a divide by zero error that will crash the system		  if TheFarm.Scenes(SceneIndex).FrameStep = 0 then		    TheFarm.Scenes(SceneIndex).FrameStep = 1		  end if		  		  // Set Scene Groups		  TheFarm.Scenes(SceneIndex).Group_Primary = Group1Popup.ListIndex+1		  TheFarm.Scenes(SceneIndex).Group_Secondary = Group2Popup.ListIndex+1		  		  // Set Check for rendered frames		  TheFarm.Scenes(SceneIndex).CheckForFrames = self.CheckForFramesBox.Value		  		  // Set Bucket Rendering Options		  //if UseBucketRenderingBox.Value then		  //TheFarm.Scenes(SceneIndex).BucketRender = True		  //else		  //TheFarm.Scenes(SceneIndex).BucketRender = False		  //end if		  		  //TheFarm.Scenes(SceneIndex).BucketRenderMaxComputers = Val(BucketComputerCountField.text)		  		  // Set Max Frame Times		  TheFarm.Scenes(SceneIndex).MaxFrameTime = Val(MaxFrameTimeField.Text)		  		  if BakeUVsBox.value then		    // Set the format		    Select Case VMAPFormatMenu.Text		    Case "Multiple PNGs"		      TheFarm.Scenes(SceneIndex).RenderType = FarmScene.rnLayeredPNGs		      TheFarm.Scenes(SceneIndex).FramesExtension = ".png"		    Case "Layered PSD"		      TheFarm.Scenes(SceneIndex).RenderType = FarmScene.rnLayeredPSD		      TheFarm.Scenes(SceneIndex).FramesExtension = ".psd"		    Case "Layered OpenEXR"		      TheFarm.Scenes(SceneIndex).RenderType = FarmScene.rnLayeredEXR		      TheFarm.Scenes(SceneIndex).FramesExtension = ".exr"		    Case "TGA"		      TheFarm.Scenes(SceneIndex).RenderType = FarmScene.rnTGA		      TheFarm.Scenes(SceneIndex).FramesExtension = ".tga"		    Case "PNG"		      TheFarm.Scenes(SceneIndex).RenderType = FarmScene.rnPNG		      TheFarm.Scenes(SceneIndex).FramesExtension = ".png"		    Case "JPG"		      TheFarm.Scenes(SceneIndex).RenderType = FarmScene.rnJPG		      TheFarm.Scenes(SceneIndex).FramesExtension = ".jpg"		    Case "EXR"		      TheFarm.Scenes(SceneIndex).RenderType = FarmScene.rnEXR		      TheFarm.Scenes(SceneIndex).FramesExtension = ".exr"		    End Select		  end if		  		  		  // Update Layered Status		  if LayeredRadioButton.value then		    if LayeredRenderingTypeList.ListIndex = 0 then		      TheFarm.Scenes(SceneIndex).RenderType = FarmScene.rnLayeredPSD		    elseif LayeredRenderingTypeList.ListIndex = 1 then		      TheFarm.Scenes(SceneIndex).RenderType = FarmScene.rnLayeredEXR		    else		      TheFarm.Scenes(SceneIndex).RenderType = FarmScene.rnLayeredPNGs		    end if		  end if		  		  		  // Should we use a custom path or not		  if UseCustomLayeredPathCheckBox.value then		    if NewOutputNameField.Text <> "" then		      TheFarm.Scenes(SceneIndex).ManualOuputPath = NewOutputNameField.Text		    end if		  end if		  		  if queued then		    TheFarm.Scenes(SceneIndex).Status = FarmScene.snQueued		  end if		  		  // We want to check for frames and see if the scene is done		  if TheFarm.Scenes(SceneIndex).CheckForRenderedFrames() then		    if TheFarm.Scenes(SceneIndex).IsDone() then		    end if		  end if		  		  QueueWindow.UpdateSceneList		End Sub	#tag EndMethod	#tag Property, Flags = &h0		SceneIndex As Int32	#tag EndProperty#tag EndWindowCode#tag Events OKandStartButton	#tag Event		Sub Action()		  if BakeUVsBox.value then		    if VMAPMenu.ListCount > 1 then		      if VMAPMenu.ListIndex =VMAPMenu.ListCount -1 then		        if not MakeNewScenePerUV(true) then		          return		        end if		      end if		    end if		  end if		  		  UpdateSceneData(true)		  QueueWindow.UpdateSceneList		  		  self.Close()		End Sub	#tag EndEvent#tag EndEvents#tag Events OKandHoldButton	#tag Event		Sub Action()		  if BakeUVsBox.value then		    if VMAPMenu.ListCount > 1 then		      if VMAPMenu.ListIndex =VMAPMenu.ListCount -1 then		        if not MakeNewScenePerUV(false) then		          return		        end if		      end if		    end if		  end if		  		  UpdateSceneData(false)		  		  self.Close		End Sub	#tag EndEvent#tag EndEvents#tag Events TabPanel1	#tag Event		Sub Open()		  TabPanel1.Remove 3		End Sub	#tag EndEvent#tag EndEvents#tag Events FirstFrameField	#tag Event		Sub LostFocus()		  if Val(me.Text) = 0 then		    me.text = "0"		  end if		End Sub	#tag EndEvent	#tag Event		Sub Open()		  me.text = "1"		End Sub	#tag EndEvent#tag EndEvents#tag Events LastFrameField	#tag Event		Sub LostFocus()		  if Val(me.Text) = 0 then		    me.text = "0"		  end if		End Sub	#tag EndEvent	#tag Event		Sub Open()		  me.text = "120"		End Sub	#tag EndEvent#tag EndEvents#tag Events FrameStepField	#tag Event		Sub LostFocus()		  if Val(me.Text) = 0 then		    me.text = "1"		  end if		End Sub	#tag EndEvent	#tag Event		Sub Open()		  me.text = "1"		End Sub	#tag EndEvent#tag EndEvents#tag Events ModoOutputsRadioButton	#tag Event		Sub Action()		  LayeredRenderingTypeList.Enabled = false		End Sub	#tag EndEvent#tag EndEvents#tag Events LayeredRadioButton	#tag Event		Sub Action()		  LayeredRenderingTypeList.Enabled = true		  		  if BakeUVsBox.value then		    MsgBox "You cannot use this option and Bake a UV Map...  The bake UVs Checkbox will be unchecked."		    BakeUVsBox.Value = false		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events BrowseButton	#tag Event		Sub Action()		  Dim f as FolderItem		  Dim fileName As String		  		  f = GetSaveFolderItem(GenericFileTypes.Etc, "Untitled")		  		  if f <> nil then		    #if TargetWin32		      // Shouldn't need this		      //fileName = Left(f.AbsolutePath, Len(f.AbsolutePath) - 4)		      		      TheFarm.Scenes(SceneIndex).ManualOuputPath = fileName		      NewOutputNameField.Text = fileName		    #else		      TheFarm.Scenes(SceneIndex).ManualOuputPath = ConvertToShellPath(f.AbsolutePath)		      NewOutputNameField.Text = ConvertToShellPath(f.AbsolutePath)		    #endif		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events UseCustomLayeredPathCheckBox	#tag Event		Sub Action()		  ModoOutputsRadioButton.Enabled = not me.value		  NewOutputNameField.Enabled = me.value		  BrowseButton.Enabled = me.value		End Sub	#tag EndEvent#tag EndEvents#tag Events Group2Popup	#tag Event		Sub Open()		  Dim i as Int32		  		  for i = 1 to 10		    me.AddRow(Str(i))		  next		  		  me.ListIndex = 0		End Sub	#tag EndEvent#tag EndEvents#tag Events Group1Popup	#tag Event		Sub Open()		  Dim i as Int32		  		  for i = 1 to 10		    me.AddRow(Str(i))		  next		  		  me.ListIndex = 0		End Sub	#tag EndEvent#tag EndEvents#tag Events BakeUVsBox	#tag Event		Sub Action()		  		  if VMAPMenu.ListCount = 0 then		    MsgBox "You must have at least 1 UV map in your scene to use this feature."		    me.value = false		    		    return		  end if		  		  		  // Make sure that the Layered option from the other tab is turned off		  if me.value = true then		    if LayeredRadioButton.Value then		      MsgBox "Layered rendering was selected under the Output tab.  Instead, please select a layered option below for UV baking."		      		      // Make sure we don't have the Layered Radio button selected		      ModoOutputsRadioButton.Value = true		    end if		  end if		  		  		  TheFarm.Scenes(SceneIndex).Bake = me.value		  		  VMAPMenu.Enabled = me.Value		  VMAPStaticText.Enabled = me.Value		  VMAPFormatMenu.Enabled = me.value		End Sub	#tag EndEvent#tag EndEvents#tag Events VMAPMenu	#tag Event		Sub Change()		  Dim i As Integer		  		  // Go through each UV Map and find the correct index		  for i = 0 to TheFarm.Scenes(SceneIndex).VMAP.Ubound		    if me.Text = TheFarm.Scenes(SceneIndex).VMAP(i) then		      TheFarm.Scenes(SceneIndex).VMAPIndex = i		    end if		  next		  		End Sub	#tag EndEvent	#tag Event		Sub Open()		  TheFarm.Scenes(SceneIndex).VMAPIndex = me.ListIndex		End Sub	#tag EndEvent#tag EndEvents#tag Events UseBucketRenderingBox	#tag Event		Sub Open()		  BucketComputerCountField.Enabled = me.Value		End Sub	#tag EndEvent	#tag Event		Sub Action()		  BucketComputerCountField.Enabled = me.Value		  BucketComputerCountField.SetFocus()		End Sub	#tag EndEvent#tag EndEvents#tag Events BucketComputerCountField	#tag Event		Sub LostFocus()		  if Val(me.Text) < 1 then		    me.text = "1"		  end if		End Sub	#tag EndEvent#tag EndEvents