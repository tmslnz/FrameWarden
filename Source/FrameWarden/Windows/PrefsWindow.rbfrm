eCountField.text = "2"		  //else		  //gDefaultBucketNodeCount = Val(DefaultBucketNodeCountField.text)		  //end if		  		  gGeometryCacheSize = CDbl(GeometryCacheSizeEditField.Text)		  		  if gGeometryCacheSize <= 0 then		    gGeometryCacheSize = 1		  end if		  		  self.close		End Sub	#tag EndEvent#tag EndEvents#tag Events PushButton2	#tag Event		Sub Action()		  		  self.close		End Sub	#tag EndEvent#tag EndEvents#tag Events PrefTabs	#tag Event		Sub Open()		  me.Remove 2		End Sub	#tag EndEvent#tag EndEvents#tag Events ChoosePathButton	#tag Event		Sub Action()		  Dim f As FolderItem		  		  f = GetOpenFolderItem("Application")		  		  if f <> nil then		    #if TargetMacOS then		      ModoPathField.text = f.ShellPath		    #elseif TargetWin32 then		      ModoPathField.text = f.AbsolutePath		    #endif		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events DefaultBucketNodeCountField	#tag Event		Sub LostFocus()		  if Val(me.text) < 2 then		    me.text = "2"		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events ChooseLicensePathButton	#tag Event		Sub Action()		  Dim f As FolderItem		  		  f = SelectFolder()		  		  if f <> nil then		    #if TargetMacOS then		      ModoLicensePathField.text = f.ShellPath		    #elseif TargetWin32 then		      ModoLicensePathField.text = f.AbsolutePath		    #endif		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events ChooseSharedFolderButton	#tag Event		Sub Action()		  Dim f As FolderItem		  		  f = SelectFolder		  		  if f <> nil then		    #if TargetMacOS then		      ModoSharedFolderField.text = f.ShellPath		    #elseif TargetWin32 then		      ModoSharedFolderField.text = f.AbsolutePath		    #endif		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events showModoProgressPopup	#tag Event		Sub Open()		  if gShowProgress then		    me.ListIndex = 0		  else		    me.ListIndex = 1		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events GeometryCacheSizeEditField	#tag Event		Sub Open()		  me.text = Str(gGeometryCacheSize)		End Sub	#tag EndEvent#tag EndEvents