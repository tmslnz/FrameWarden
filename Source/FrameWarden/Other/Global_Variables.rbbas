#tag ModuleProtected Module Global_Variables	#tag Method, Flags = &h0		Sub ClearLog()		  LogWindow.LogField.text = ""		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function ConvertToShellPath(oldPath As String) As String		  Dim shellPath As String		  		  shellPath = ReplaceAll(oldPath, ":", "/")		  		  shellPath = "/Volumes/" + shellPath		  		  return shellPath		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function GetChannelType(code As Int32) As String		  select case code		  case 26, 29, 33		    return "X"		    		  case 27, 30, 34		    return "Y"		    		  case 28, 31, 35		    return "Z"		    		  end select		  		  return "[Unknown: " + Str(code) + "]"		End Function	#tag EndMethod	#tag Method, Flags = &h0		Sub UpdateLicenseInfo(newKey As String)		  gMaxNodes = 2		  		  if gLicenseKey.CheckKey(newKey) = Int32(LicenseTypes.Free) then		    gLicenseType = Int32(LicenseTypes.Free)		    gLicenseKey.Key = newKey		    QueueWindow.Title = "Frame Warden - Free Version"		    return		  end if		  		  		  gLicenseType = gLicenseKey.CheckKey(newKey)		  		  if gLicenseType =  Int32(LicenseTypes.Ten) then		    QueueWindow.Title = "Frame Warden - Ten Node Version"		    gLicenseKey.Key = newKey		    gMaxNodes = 10		  elseif gLicenseType =  Int32(LicenseTypes.Unlimited) then		    QueueWindow.Title = "Frame Warden - Unlimited Node Version"		    		    gLicenseKey.Key = newKey		    gMaxNodes = -1		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub WriteToLog(str As String, mode as String)		  if gLogEnabled = true then		    if mode = gLogMode then		      if LogWindow.LogField.text <> "" then		        LogWindow.LogField.text = LogWindow.LogField.text + Chr(13)		      end if		      		      LogWindow.LogField.text = LogWindow.LogField.text + str		    end if		  end if		End Sub	#tag EndMethod	#tag Property, Flags = &h0		gAgreeToLicense As Boolean = False	#tag EndProperty	#tag Property, Flags = &h0		gBakeBorderSize As Integer = 5	#tag EndProperty	#tag Property, Flags = &h0		gClipboard As Clipboard	#tag EndProperty	#tag Property, Flags = &h0		gDefaultBucketNodeCount As Integer	#tag EndProperty	#tag Property, Flags = &h0		gDefaultMaxFrameTime As Integer	#tag EndProperty	#tag Property, Flags = &h0		gGeometryCacheSize As Integer	#tag EndProperty	#tag Property, Flags = &h0		gLicenseKey As LicenseKey_Class	#tag EndProperty	#tag Property, Flags = &h0		gLicenseType As Integer	#tag EndProperty	#tag Property, Flags = &h0		gLogClearAfterMinutes As Integer = 10	#tag EndProperty	#tag Property, Flags = &h0		gLogMode As String	#tag EndProperty	#tag Property, Flags = &h0		gMaxNodes As Integer	#tag EndProperty	#tag Property, Flags = &h0		gModoLicensePath As String	#tag EndProperty	#tag Property, Flags = &h0		gModoPath As String	#tag EndProperty	#tag Property, Flags = &h0		gNoResponseRelaunchTime As Integer	#tag EndProperty	#tag Property, Flags = &h0		gSharedFolder As String	#tag EndProperty	#tag Property, Flags = &h0		gShowProgress As Boolean = true	#tag EndProperty	#tag Property, Flags = &h0		ModoCommandsFolder As FolderItem	#tag EndProperty	#tag Property, Flags = &h0		MyIPAddress As String	#tag EndProperty	#tag Property, Flags = &h0		PrefsFile As FolderItem	#tag EndProperty	#tag Property, Flags = &h0		programStartupTime As Integer	#tag EndProperty	#tag Property, Flags = &h0		TheFarm As Farm	#tag EndProperty	#tag Constant, Name = gLaunchRetryTime, Type = Double, Dynamic = False, Default = \"1800", Scope = Public	#tag EndConstant	#tag Constant, Name = gLogEnabled, Type = Boolean, Dynamic = False, Default = \"False", Scope = Public	#tag EndConstant	#tag Constant, Name = gNilString, Type = String, Dynamic = False, Default = \"!/\\\?", Scope = Public	#tag EndConstant	#tag Constant, Name = gNoFrameToRender, Type = Double, Dynamic = False, Default = \"-9999999", Scope = Public	#tag