ger(LicenseTypes.Ten) then		      MsgBox "Bad key generated! Iteration: " + Str(i)		      return		    end if		  next		  		  MsgBox "1,000,000 Generated good!"		End Sub	#tag EndEvent#tag EndEvents#tag Events Check100_Unl	#tag Event		Sub Action()		  Dim i As Integer		  		  for i = 1 to 1000000		    GenUnlimitedLicense		    		    if CheckKey(LicenseKey.Key) <> Integer(LicenseTypes.Unlimited) then		      MsgBox "Bad key generated! Iteration: " + Str(i)		      return		    end if		  next		  		  MsgBox "1,000,000 Generated good!"		End Sub	#tag EndEvent#tag EndEvents